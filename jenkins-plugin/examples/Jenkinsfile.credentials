// Example using Jenkins credentials for security

pipeline {
    agent any
    
    environment {
        DEVLOGS_URL = credentials('devlogs-opensearch-url')
    }
    
    stages {
        stage('Build and Test') {
            steps {
                devlogs(url: env.DEVLOGS_URL) {
                    sh 'npm install'
                    sh 'npm test'
                }
            }
        }
    }
}
