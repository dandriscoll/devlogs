// Simple example - basic pipeline-level usage

pipeline {
    agent any

    options {
        devlogs(url: 'https://admin:password@opensearch.example.com:9200/devlogs-myproject')
    }

    stages {
        stage('Build') {
            steps {
                sh 'echo "Hello from devlogs!"'
                sh 'make build'
            }
        }
    }
}
